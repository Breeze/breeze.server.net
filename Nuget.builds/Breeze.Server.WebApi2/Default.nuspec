    <metadata>
        <id>{{id}}</id>
        <version>{{version}}</version>
        <title>Breeze Server - for Web API 2.2</title>
        <authors>Jay Traband</authors>
        <owners>IdeaBlade</owners>
        <licenseUrl>http://breeze.github.io/doc-main/license.html</licenseUrl>
